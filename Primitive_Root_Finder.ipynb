   ],
   "source": [
    "n = 229\n",
    "R = Integers(n)\n",
    "primitive_roots = []\n",
    "for a in range(1, n):\n",
    "    e = R(a)\n",
    "    if e.multiplicative_order() == euler_phi(n):\n",
    "        primitive_roots.append(a)\n",
    "print(\"Primitive roots modulo 11:\", primitive_roots)\n",
    "print(\"Number of primitive roots:\", len(primitive_roots))\n"
   ]
